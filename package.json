{
  "name": "react-admin",
  "version": "0.0.0",
  "private": true,
  "author": {
    "name": "吧啦哔哩啵",
    "email": "balabilibo@163.com",
    "url": "https://gitee.com/balabilibo/nest-server/tree/react-admin/"
  },
  "type": "module",
  "scripts": {
    "build": "vite build",
    "commit": "git-cz",
    "dev": "vite",
    "lint": "eslint .",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "prepare": "husky",
    "preview": "vite preview"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@ant-design/icons": "5.x",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@types/react-transition-group": "^4.4.12",
    "ahooks": "^3.8.4",
    "antd": "^5.24.9",
    "axios": "^1.9.0",
    "echarts": "^5.6.0",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pkg-types": "^2.1.0",
    "react": "^19.0.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.5.3",
    "react-transition-group": "^4.4.5",
    "typeit-react": "^2.7.8",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.22.0",
    "@iconify/react": "^6.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.1",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "commitizen": "^4.3.1",
    "cz-git": "^1.11.1",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "less": "^4.3.0",
    "lint-staged": "^16.0.0",
    "prettier": "3.5.3",
    "prettier-plugin-packagejson": "^2.5.13",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "unocss": "^66.1.0",
    "vite": "^6.3.1",
    "vite-plugin-fake-server": "^2.2.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-svg-icons": "^2.0.1"
  }
}
